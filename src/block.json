{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gutenberg-ts-utils/typed-attributes-demo",
	"version": "0.1.0",
	"title": "Typed Attributes Demo",
	"category": "widgets",
	"icon": "admin-tools",
	"description": "A simple block demonstrating dynamically typed block attributes",
	"attributes": {
		"lightThemeBgColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"darkThemeBgColor": {
			"type": "string",
			"default": "#000000"
		},
		"lightThemeTextColor": {
			"type": "string",
			"default": "#000000"
		},
		"darkThemeTextColor": {
			"type": "string",
			"default": "#ffffff"
		}
	},
	"example": {},
	"supports": {
		"interactivity": true
	},
	"textdomain": "typed-attributes-demo",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.ts"
}
