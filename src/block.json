{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gutenberg-ts-utils/typed-interactive-block",
	"version": "0.1.0",
	"title": "Interactive Theme Switcher",
	"category": "widgets",
	"icon": "admin-tools",
	"description": "Switches between light and dark themes using attributes and the Interactivity API, demonstrating automated type generation.",
	"attributes": {
		"lightThemeBgColor": {
			"type": "string",
			"default": "#fff"
		},
		"darkThemeBgColor": {
			"type": "string",
			"default": "#333"
		},
		"lightThemeTextColor": {
			"type": "string",
			"default": "#000"
		},
		"darkThemeTextColor": {
			"type": "string",
			"default": "#fff"
		}
	},
	"example": {},
	"supports": {
		"interactivity": true
	},
	"textdomain": "typed-attributes-demo",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.ts"
}
